  <icon src="res/icons/ios/icon-small@3x.png"/>
    <plugin name="com.phonegap.plugins.facebookconnect" spec="https://github.com/Telerik-Verified-Plugins/Facebook">
        <variable name="APP_ID" value="454580491577152" />
        <variable name="APP_NAME" value="Myna" />
    </plugin>
    <plugin name="cordova-plugin-x-socialsharing" spec="https://github.com/EddyVerbruggen/SocialSharing-PhoneGap-Plugin.git" />
    <plugin name="ionic-plugin-keyboard" spec="~2.2.1" />
    <allow-navigation href="*" />
    <plugin name="cordova-plugin-firebase-messaging" spec="~0.12.0" />
  <plugin name="cordova-plugin-firebase" spec="~0.1.25"/>
  <preference name="SplashShowOnlyFirstTime" value="false"/>
  <engine name="android" spec="~5.1.1"/>
</widget>
